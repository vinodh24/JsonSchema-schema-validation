{
	"$schema":"http://json-schema.org/draft-04/schema#",
	"type":"object",
	"properties":{
		"clli":{
			"type":"string",
			"minLength":1
		},
		"cuid":{
			"type":"string"
		},
		"serviceOrder":{
			"type":"string"
		},
		"telephoneNumber":{
			"type":"string",
			"pattern":"^[0-9]{10}$"
		},
		"deviceIP":{
			"type":"string",
			"oneOf":[
				{
					"format":"ipv4"
				}
			]
		},
		"scp":{
			"type":"array",
			"minItems":1,
			"maxItems":2,
			"items":[
				{
					"type":"string",
					"minLength":1
				}
			]
		},
		"vendor":{
			"type":"string",
			"minLength":1
		},
		"model":{
			"type":"string",
			"minLength":1
		},
		"pon":{
			"type":"object",
			"properties":{
				"ontId":{
					"type":"string"
				},
				"ontPort":{
					"type":"string"
				}
			},
			"required":[
				"ontId",
				"ontPort"
			]
		}
	},
	"required":[
		"clli",
		"scp",
		"vendor",
		"model"
	]
}