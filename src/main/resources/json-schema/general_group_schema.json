{
	"$schema":"http://json-schema.org/draft-04/schema#",
	"type":"object",
	"properties":{
		"clli":{
			"type":"string",
			"minLength":1
		},
		"serviceOrder":{
			"type":"string"
		},
		"cuid":{
			"type":"string"
		},
		"deviceIP":{
			"anyOf":[
				{
					"type":"string",
					"maxLength":0
				},
				{
					"type":"string",
					"format":"ipv4"
				}
			]
		},
		"scp":{
			"type":"array",
			"minItems":1,
			"maxItems":2,
			"items":[
				{
					"type":"string",
					"minLength":1
				}
			]
		},
		"vendor":{
			"type":"string",
			"minLength":1
		},
		"model":{
			"type":"string",
			"minLength":1
		},
		"telephoneNumber":{
			"type":"string",
			"minLength":1,
			"pattern":"^[0-9]{10}$"
		},
		"cos":{
			"type":"string",
			"minLength":1
		},
		"assignedTech":{
			"type":"string",
			"minLength":1
		}
	},
	"required":[
		"clli",
		"scp",
		"vendor",
		"model",
		"telephoneNumber",
		"cos",
		"assignedTech"
	]
}