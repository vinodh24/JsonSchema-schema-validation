{
	"$schema":"http://json-schema.org/draft-04/schema#",
	"type":"object",
	"properties":{
		"clli":{
			"type":"string"
		},
		"deviceIP":{
			"type":"string"
		},
		"scp":{
			"type":"array",
			"items":[
				{
					"type":"string"
				}
			]
		},
		"vendor":{
			"type":"string"
		},
		"model":{
			"type":"string"
		},
		"telephoneNumber":{
			"type":"string"
		},
		"cos":{
			"type":"string"
		},
		"assignedTech":{
			"type":"string"
		},
		"pon":{
			"type":"object",
			"properties":{
				"isPon":{
					"type":"boolean"
				},
				"ontId":{
					"type":"string",
					"minLength":1
				},
				"ontPort":{
					"type":"string",
					"minLength":1
				}
			}
		},
		"data":{
			"type":"object",
			"properties":{
				"isData":{
					"type":"boolean"
				},
				"sTag":{
					"type":"string"
				},
				"cTag":{
					"type":"string"
				},
				"dataCos":{
					"type":"string"
				}
			},
			"required":[
				"isData",
				"sTag",
				"cTag",
				"dataCos"
			]
		},
		"video":{
			"type":"object",
			"properties":{
				"isVideo":{
					"type":"boolean"
				},
				"videoCos":{
					"type":"string"
				}
			},
			"required":[
				"isVideo",
				"videoCos"
			]
		},
		"voice":{
			"type":"object",
			"properties":{
				"isVoice":{
					"type":"boolean"
				},
				"type":{
					"type":"string"
				},
				"crv":{
					"type":"string"
				}
			},
			"required":[
				"isVoice",
				"type",
				"crv"
			]
		}
	},
	"required":[
		"assignedTech",
		"pon",
		"data",
		"video",
		"voice"
	]
}